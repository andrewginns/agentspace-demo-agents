# GCloud
PROJECT_ID = "your-project-id"
PROJECT_NUMBER = "your-project-number"
LOCATION = "us-central1"
STAGING_BUCKET = "gs://your-staging-bucket"

# Agentspace
APP_ID = "your-agentspace-app-id"
AS_LOCATION = "global"  # AgentSpace location (e.g., global, eu, us)

# ADK Deployed Agent running in Agent Engine
ADK_DEPLOYMENT_ID = "your-agent-number"
AE_LOCATION = "global"  # Location where the agent engine is deployed

# Set vertexai backend
GOOGLE_GENAI_USE_VERTEXAI="True"

# Agent Configuration in Agentspace
AGENT_DISPLAY_NAME = "agent-name"
AGENT_DESCRIPTION = "Agent description"

# OAuth Configuration (Optional - only needed if agent requires user authorization)
AUTH_ID = "your-auth-id"  # Authorization resource ID
OAUTH_CLIENT_ID = "your-oauth-client-id"
OAUTH_CLIENT_SECRET = "your-oauth-client-secret"
OAUTH_AUTH_URI = "https://accounts.google.com/o/oauth2/v2/auth?client_id=CLIENT_ID&scope=SCOPES&include_granted_scopes=true&response_type=code&access_type=offline&prompt=consent"
OAUTH_TOKEN_URI = "https://oauth2.googleapis.com/token"
